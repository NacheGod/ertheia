    <!--
		Ruler's Authority - Max CP/HP/MP + 1000, Weight + 25600, Damage + 8%, Received Damage - 9%, Damage Reflect Resistance + 5%, Inventory Slot + 36, and all Attribute Resistance + 15.

	-->
    <skill id="15145" levels="1" name="Ruler's Authority">
		<set name="icon" value="icon.skill0332"/>
		<set name="target" value="TARGET_SELF"/>
		<set name="skillType" value="BUFF"/>
		<set name="operateType" value="OP_PASSIVE"/>
		<for>
			<add order="0x40" stat="maxHp" value="1000"/>
			<add order="0x40" stat="maxMp" value="1000"/>
			<add order="0x40" stat="maxCp" value="1000"/>
			<add order="0x40" stat="maxLoad" value="25600"/>
			<add order="0x40" stat="inventoryLimit" value="36"/>
			<add order="0x40" stat="defenceFire" value="15"/>
			<add order="0x40" stat="defenceWater" value="15"/>
			<add order="0x40" stat="defenceWind" value="15"/>
			<add order="0x40" stat="defenceEarth" value="15"/>
			<add order="0x40" stat="defenceHoly" value="15"/>
			<add order="0x40" stat="defenceUnholy" value="15"/>
			<mul order="0x30" stat="pvpPhysDefenceBonus" value="1.09"/> <!-- получаемый урон в PvP -9% -->
			<mul order="0x30" stat="pvpPhysSkillDefenceBonus" value="1.09"/> <!-- получаемый урон в PvP -9% -->
			<mul order="0x30" stat="pvpMagicSkillDefenceBonus" value="1.09"/> <!-- получаемый урон в PvP -9% -->
